{
  "version": 2,
  "extra_config": {
    "github_com/devopsfaith/krakend-gologging": {
      "level": "WARNING",
      "prefix": "[KRAKEND]",
      "syslog": false,
      "stdout": true
    }
  },
  "endpoints": [
    {
      "endpoint": "/hello",
      "method": "GET",
      "backend": [
        {
          "url_pattern": "/hello",
          "encoding": "json",
          "host": [
            "http://172.17.0.1:1378"
          ],
          "extra_config": {
            "github.com/devopsfaith/krakend-martian": {
              "header.Modifier": {
                "scope": ["request"],
                "name": "X-From",
                "value": "parham"
              }
            }
          }
        }
      ]
    }
  ]
}
